{
    "name": "webwriter",
    "version": "1",
    "manifest_version": 2,
    "description": "Markdown editor",
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "default_locale": "en",
    "app": {
        "background": {
          "scripts": ["scripts/main.js"]
        }
    },
    "permissions": [
      {"fileSystem": ["write"]}
    ]
}
